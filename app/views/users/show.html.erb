<div class="container main-container page-container">
  <div class="my_panel">
	 	<div class="profile-box">
		  <%= image_tag @user.profile.image %><br>
		  <h2><%= @user.nickname %></h2>
		  <p><%= link_to @user.profile.website, @user.profile.website %></p>
		  <p><%= link_to '<p><i class="fa fa-github"></i> Github</p>'.html_safe, @user.profile.github %></p>
		  <p><%= link_to '<p><i class="fa fa-facebook"></i> Facebook</p>'.html_safe, @user.profile.facebook %></p>
		  <p><%= link_to '<p><i class="fa fa-twitter"></i> Twitter</p>'.html_safe, @user.profile.twitter %></p>
		  <p><%= link_to '<p><i class="fa fa-linkedin"></i> Linked In</p>'.html_safe, @user.profile.linkedin %></p>
	  </div>

	  <div class="bio">
			<h2>About Me</h2>
		 	<p><%= @user.profile.bio %></p>
		 	<h2>Founder</h2>
		 	<% @user.projects.each do |project| %>
		 	 	<h4><%= link_to project.name, project_path(project) %></h4>
		 	 	<p><%= project.description %></p>
		 	<% end %>
		 	<h2>Contributor</h2>
			<% @user.all_following.each do |project| %>
				<h4><%= link_to project.name, project_path(project) %></h4>
		 	 	<p><%= project.description %></p>
		 	<% end %>
	 	</div>

	 	
 	</div>
</div>

